#include <stdio.h>
/**
 * add - Adds two integers
 * @a: first integer
 * @b: second integer
 *
 * Return: The sum of a and b
 */

int add(int a, int b) {
    return a + b;
}

int sub(int a, int b) {
    return a - b;
}

int mul(int a, int b) {
    return a * b;
}

int div(int a, int b) {
    return a / b;
}

int mod(int a, int b) {
    return a % b;
}

int main() {
    printf("This is a shared library.\n");
    return 0;
}

